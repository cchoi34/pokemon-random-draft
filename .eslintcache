ps = {\n  pokemonName: string,\n}\n\nfunction PokemonSprite({ pokemonName }: PokemonSpriteProps) {\n  const [imageSrc, setImageSrc] = useState('');\n  const [imageAlt, setImageAlt] = useState('');\n\n  function getImage(path: string) {\n    async function getImageFromStorage() {\n      try {\n        const storageRef = storage.ref();\n        const imageRef = storageRef.child(path);\n        const url = await imageRef.getDownloadURL();\n        setImageSrc(url);\n        setImageAlt(pokemonName);\n      } catch (e) {\n        console.log(e);\n      }\n    }\n    getImageFromStorage();\n  }\n\n  function getPath() {\n    const lowerCasePokemon = pokemonName.toLowerCase();\n    return `${POKEMON_STORAGE_PATH}/${lowerCasePokemon}.png`;\n  }\n\n  useEffect(() => {\n    getImage(getPath());\n  }, []);\n\n  return (\n    <img\n      src={imageSrc}\n      alt={imageAlt}\n    />\n  );\n}\n\nexport default PokemonSprite;","/Users/christopherchoi/dev_projects/pokemon-draft-randomizer/pokemon-draft-randomizer-app/src/utils/dataTypes.tsx",[],["121","122","123","124","125","126"],"/Users/christopherchoi/dev_projects/pokemon-draft-randomizer/pokemon-draft-randomizer-app/src/components/basic-elements/PokemonType.tsx",[],"/Users/christopherchoi/dev_projects/pokemon-draft-randomizer/pokemon-draft-randomizer-app/src/components/NotFound404.tsx",[],"/Users/christopherchoi/dev_projects/pokemon-draft-randomizer/pokemon-draft-randomizer-app/src/components/Footer.tsx",[],"/Users/christopherchoi/dev_projects/pokemon-draft-randomizer/pokemon-draft-randomizer-app/src/utils/draftUtils.tsx",[],"/Users/christopherchoi/dev_projects/pokemon-draft-randomizer/pokemon-draft-randomizer-app/src/components/YourPokemon.tsx",[],"/Users/christopherchoi/dev_projects/pokemon-draft-randomizer/pokemon-draft-randomizer-app/src/utils/firestoreUtils.tsx",[],"/Users/christopherchoi/dev_projects/pokemon-draft-randomizer/pokemon-draft-randomizer-app/src/components/basic-elements/PokemonPreviewCard.tsx",[],{"ruleId":